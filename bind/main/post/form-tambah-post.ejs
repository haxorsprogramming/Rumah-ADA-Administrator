<div class="row" id="divTambahArtikel">
  <div class="col-lg-12">
    <div class="card">
      <div class="card-body">
        <h4 class="header-title">Isi data artikel baru</h4>
        <br />
        <div class="row">
          <div class="col-6 col-lg-6">
            <div class="form-group">
              <label>Judul</label>
              <input type="text" class="form-control" id="txtJudul" onkeyup="setSlug()"/>
            </div>
            <div class="form-group">
              <label>Slug</label>
              <input type="text" class="form-control" id="txtSlug" />
            </div>
          </div>
          <div class="col-6 col-lg-6">
            <div class="form-group">
              <label>Writer</label>
              <input type="text" class="form-control" id="txtWriter" />
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-12 col-lg-12">
            <div class="form-group">
              <label>Isi Artikel</label>
              <div id="editor">
                <p>This is some sample content.</p>
              </div>
            </div>
            <div>
                <a href="javascript:void(0)" class="btn btn-primary" @click="prosesAddArtikel()">Proses Tambah Artikel</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="/base/js/tambah-post.js"></script>
